<%projects=@projects%>
<div class="l-panel-contain">
  <div class="l-panel">
    <%=link_to_all_tasks%>
    <%=link_to_my_tasks%>
    <%=link_to_starred_tasks%>
    <a href="#" class="completed"><span class="icon"></span>Completed</a>
    
    <div class="projects-list">
      <%projects.each do |project|%>
        <div class="project">
          <span class="icon"></span>
          <span class="project-title">
            <span class="icon"></span>
            <%=project.name%>
          </span>
          <ul>
            <%project.task_lists.each do |task_list|%>
            <li><span><%=task_list.name%></span></li>
            <%end%>
          </ul>
          <div class="clear-fix"></div>
        </div>
      <%end%>
    </div>
  </div>
</div>
