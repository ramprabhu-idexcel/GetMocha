				    </fieldset>

				    <fieldset>
						<div class="label-contain">
							<label>Title:</label>
						</div>
						<div class="info-right">
							<span class="info">Founder</span><a href="/settings-profile-1" class="edit">Edit</a>
				       		<input type="text" name="title" value="" class="textfield hidden" />

						</div>
				    </fieldset>
				    <fieldset>
						<div class="label-contain">
							<label>Primary:</label>
						</div>
						<div class="info-right">
							<span class="info">jessema@gmail.com</span><a href="/settings-profile-1" class="edit">Edit</a>

				       		<input type="text" name="primary-email" value="" class="textfield hidden" />
				       	</div>
				    </fieldset>
				    <fieldset>
						<div class="label-contain">
							<label>Secondary:</label>
						</div>
						<div class="info-right">

							<span class="info">jessema@me.com</span><a href="/settings-profile-1" class="edit">Edit</a>
				       		<input type="text" name="secondary-email" value="" class="textfield hidden" /><br />
