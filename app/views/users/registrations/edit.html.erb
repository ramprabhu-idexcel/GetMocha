<h2>Sign up</h2>
<div class="l-panel-contain">
		<div class="l-panel">
		  
			<div class="sub-header">Personal Settings</div>
			<a href="/settings-profile" class="my-account open" onclick="addClass(this);"><span class="icon"></span>My Account</a>
		</div>
	</div>
	<div class="m-panel-contain">
		<div class="m-panel">
			<a href="#" class="m-tab open" id="myprofile1">My Profile</a>
		  <a href="#" class="m-tab alt" id="myprofile2">My Profile</a>

			<a href="#" class="m-tab alt" id="mycontact1">My Contacts</a>
		 <a href="#" class="m-tab open" id="mycontact2">My Contacts</a>

		</div>
	</div>
	<div class="r-panel-contain">
		<div class="r-panel">
			<div class="avatar-upload">
				<div class="avatar-lg"></div>
				<img src="/images/content/avatar-lg.jpg" class="avatar-lg-img" alt="avatar"/>
				<div class="drag-drop">Drag and drop files here</div>
				<div class="clear-fix"></div>
			</div>
			<div class="user-info" id="user_information">
				<form action="">
					<fieldset>
						<div class="label-contain">
							<p><%= label_tag "First Name:"%><br /></p>
						</div>
						<div class="info-right">
							<span class="info_firstname" id="label_first_name"><%=current_user.first_name%></span>
							 <%=text_field :val,:data, :id =>"txt_firstname",:value=>current_user.first_name%>
						<%=link_to "Edit","#",:class=>"edit",:id=>"first_name"%>
						<%=link_to "Save","#",:class=>"edit",:id=>"save_firstname"%>
			       		</div>
				    </fieldset>
				    <fieldset>
						<div class="label-contain">
							<p><%= label_tag "Last Name:"%><br /></p>
						</div>
						<div class="info-right">
							<span class="info_lastname" id="label_last_name"><%=current_user.last_name%></span>
    							 <%=text_field :val,:data, :id =>"txt_lastname",:value=>current_user.last_name%>
                   <%=link_to "Edit","#",:class=>"edit",:id=>"last_name"%>
									 <%=link_to "Save","#",:class=>"edit",:id=>"save_lastname"%>
				       	</div>
				    </fieldset>
				    <fieldset>
						<div class="label-contain">
							<p><%= label_tag "Title:"%><br /></p>
						</div>
						<div class="info-right">
							<span class="info_title" id="label_title"><%=current_user.title%></span>
							<%=text_field :val,:data, :id =>"txt_title",:value=>current_user.title%>
              <%=link_to "Edit","#",:class=>"edit",:id=>"title"%>
						 <%=link_to "Save","#",:class=>"edit",:id=>"save_title"%>
				    </div>
				    </fieldset>
				    <fieldset>
						<div class="label-contain">
							<p><%= label_tag "Primary:"%><br /></p>
						</div>
						<div class="info-right">
							<span class="info_email" id="label_email"><%=current_user.email%></span>
						 <%=text_field :val,:data, :id =>"txt_email",:value=>current_user.email%>
               <%=link_to "Edit","#",:class=>"edit",:id=>"email"%>
							<%=link_to "Save","#",:class=>"edit",:id=>"save_email"%>
				       	</div>
				    </fieldset>
				    <fieldset>
						<div class="label-contain">
							<p><%= label_tag "Secondary:"%><br /></p>
						</div>
						<div class="info-right">
							<span class="info"> </span><%=link_to "Edit","#",:class=>"edit"%>
				       		<input type="text" name="secondary-email" value="" class="textfield hidden" /><br />
				       		<a href="#" class="add-email">+ Add another email</a>
				       	</div>
				    </fieldset>
				    <fieldset>
						<div class="label-contain">
							<p><%= label_tag "Password:"%><br /></p>
						</div>
						<div class="info-right">
							<span class="info_password" id="label_password">********</span>
							 <%=password_field :val,:data, :id =>"txt_password",:value=>"********"%>
							<%=link_to "Edit","#",:class=>"edit",:id=>"password"%>
							<%=link_to "Save","#",:class=>"edit",:id=>"save_password"%>
				       	</div>
				    </fieldset>
				    <fieldset>
						<div class="label-contain">
							<p><%= label_tag "Phone:"%><br /></p>
						</div>
						<div class="info-right">
							<span class="info_phone" id="label_phone"><%=current_user.phone%></span>
						 <%=text_field :val,:data, :id =>"txt_phone",:value=>current_user.phone%>
               <%=link_to "Edit","#",:class=>"edit",:id=>"phone"%>
							<%=link_to "Save","#",:class=>"edit",:id=>"save_phone"%>
				       	</div>
				    </fieldset>
				    <fieldset>
						<div class="label-contain">
							<p><%= label_tag "Mobile:"%><br /></p>
						</div>
						<div class="info-right">
							<span class="info_mobile" id="label_mobile"><%=current_user.mobile%></span>
						 <%=text_field :val,:data, :id =>"txt_mobile",:value=>current_user.mobile%>
               <%=link_to "Edit","#",:class=>"edit",:id=>"mobile"%>
							<%=link_to "Save","#",:class=>"edit",:id=>"save_mobile"%>
				       	</div>
				    </fieldset>
				    <fieldset>
						<div class="label-contain">
							<p><%= label_tag "Time Zone:"%><br /></p>
						</div>
						<div class="info-right">
							<span class="info_time_zone" id="label_time_zone"><%=current_user.time_zone%></span>
						  <%=text_field :val,:data, :id =>"txt_time_zone",:value=>current_user.time_zone%>
               <%=link_to "Edit","#",:class=>"edit",:id=>"time_zone"%>
							<%=link_to "Save","#",:class=>"edit",:id=>"save_time_zone"%>
				       	</div>
				    </fieldset>
				    <fieldset>
						<div class="label-contain">
							<p><%= label_tag "Chat Color:"%><br /></p>
						</div>
						<div class="info-right">
			       <p><%= text_field :color,:data,:size =>1,:id =>'choose_color' %>
							<img src="/images/color-chooser.png" alt="color-choose" id="colorSelector" /></p>
						 </div>
				    </fieldset>
				</form>
			</div>
		</div>
	</div>

<script>
var Edit=true;

</script>


