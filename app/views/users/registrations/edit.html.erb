							<span class="info">********</span><a href="/settings-profile-1" class="edit">Edit</a>
				       		<input type="password" name="password" value="" class="textfield hidden" />
				       	</div>
				    </fieldset>

				    <fieldset>
						<div class="label-contain">
							<label>Phone:</label>
						</div>
						<div class="info-right">
							<span class="info">(415) 685-7782</span><a href="/settings-profile-1" class="edit">Edit</a>
				       		<input type="text" name="phone" value="" class="textfield hidden" />

				       	</div>
				    </fieldset>
				    <fieldset>
						<div class="label-contain">
							<label>Mobile:</label>
						</div>
						<div class="info-right">
							<span class="info">(778) 682-1723</span><a href="/settings-profile-1" class="edit">Edit</a>

